{
  "installCommand": "corepack enable && corepack prepare pnpm@9.15.0 --activate && pnpm install",
  "buildCommand": "pnpm build",
  "outputDirectory": ".vercel/output",
  "crons": [
    {
      "path": "/api/sync/renew-webhooks",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
